    @Html.ActionLink("No_perdidas", "Perfil", "Estadisticas", null, new { @class = "nav-link" })
    @Html.ActionLink("No_perdidas", "Perfil", "Estadisticas", null, new { @class = "nav-link" })
    @Html.ActionLink("No_torneo Ganados", "Perfil", "Estadisticas", null, new { @class = "nav-link" })
    @Html.ActionLink("No_torneo Pts", "Perfil", "Estadisticas", null, new { @class = "nav-link" })
</div>